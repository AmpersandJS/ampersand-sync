{
  "name": "ampersand-sync",
  "description": "Provides sync behavior for updating data from ampersand models and collections to the server. With a flavor of promise.",
  "version": "3.0.3",
  "author": [
    "Tri Nguyen <tri@tridnguyen.com>",
    "Henrik Joreteg <henrik@andyet.net>"
  ],
  "browserify": {
    "transform": [
      "ampersand-version"
    ]
  },
  "bugs": {
    "url": "https://github.com/tnguyen14/ampersand-sync-with-promise/issues"
  },
  "dependencies": {
    "promise": "^5.0.0",
    "ampersand-version": "^1.0.0",
    "qs": "^2.2.4",
    "underscore": "~1.6.0",
    "xhr": "^1.10.0"
  },
  "devDependencies": {
    "ampersand-model": "^4.0.1",
    "ampersand-rest-collection": "^2.0.1",
    "browserify": "^6.2.0",
    "jshint": "^2.5.3",
    "phantomjs": "^1.9.12",
    "precommit-hook": "^1.0.7",
    "run-browser": "~1.3.0",
    "tap-spec": "^1.0.1",
    "tape": "^2.14.0",
    "tape-run": "^0.3.0"
  },
  "homepage": "https://github.com/tnguyen14/ampersand-sync-with-promise",
  "keywords": [
    "backbone",
    "sync",
    "rest",
    "ampersand"
  ],
  "license": "MIT",
  "main": "ampersand-sync.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tnguyen14/ampersand-sync-with-promise.git"
  },
  "scripts": {
    "start": "run-browser test/*",
    "test": "browserify test/* | tape-run | tap-spec"
  },
  "testling": {
